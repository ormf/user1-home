:output '((:data) (:lilypond :process t :view nil))

:lilypond-exe "/usr/bin/lilypond"
:lilypond-opts '("--pdf")
:lilypond-out-ext "pdf"

:lilypond-view-exe "/usr/bin/evince"
:cmn-view-exe "/usr/bin/evince"

:verbose 1
:min-tuplet-dur 1
:max-tuplet-dur 1
:max-tuplet '(5)
:caut-acc-ottavas t
:auto-timesig-comp nil
:auto-beams nil
:auto-override-timesigs nil
